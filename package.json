{
  "name": "openair-syntax-highlighting",
  "displayName": "openair_syntax_highlighting",
  "description": "Syntax Highlighting for OpenAir airsapce format",
  "repository": "https://github.com/llauner/openair-syntax-highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "openair",
      "aliases": ["OpenAir", "openair"],
      "extensions": [".txt",".openair"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "openair",
      "scopeName": "source.openair",
      "path": "./syntaxes/openair.tmLanguage.json"
    }]
  }
}
